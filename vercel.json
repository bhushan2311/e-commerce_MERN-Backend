{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src": "/products/",
      "dest": ""
    },
    {
      "src": "/categories",
      "dest": "index.js"
    },
    {
      "src": "/categories/",
      "dest": ""
    },
    {
      "src": "/brands",
      "dest": "index.js"
    },
    {
      "src": "/brands/",
      "dest": ""
    },
    {
      "src": "/users",
      "dest": "index.js"
    },
    {
      "src": "/users/",
      "dest": ""
    },
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/auth/",
      "dest": ""
    },
    {
      "src": "/cart",
      "dest": "index.js"
    },
    {
      "src": "/cart/",
      "dest": ""
    },
    {
      "src": "/orders",
      "dest": "index.js"
    },
    {
      "src": "/orders/",
      "dest": ""
    }
  ]
}
